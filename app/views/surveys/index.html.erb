<h1><strong>View <%= pluralize(@surveys.count, "Survey") %></strong></h1>
<table class="table table-warning table-bordered border-dark table-striped">
	<thead>
		<th scope="col">Survey Name</th>
		<th scope="col">Actions</th>
	</thead>
	<tbody>
		<% @surveys.each do |survey| %>
		<tr>
			<td><%= survey.survey_name %></td>
			<td>
				<a href="/surveys/<%= survey.id%>/edit">Edit</a> |
				<a href="/surveys/<%= survey.id%>/delete">Delete</a> |
				<a href="/surveys/<%= survey.id%>">Survey Questions</a> |


				<a href="/respondent_survey/<%= survey.id%>">Respondent Surveys</a>

			</td>
		</tr>
		<% end %>
	</tbody>
</table>
