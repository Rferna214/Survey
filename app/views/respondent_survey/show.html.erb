<h1/><strong>View Respondents</strong> </h1>
<table class="table table-warning table-bordered border-dark table-striped">
	<thead>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
			</thead>
	<tbody>
		<% @respondent.each do |respondent| %>
		<tr>
			<td><%= respondent.first_name %></td>
			<td><%= respondent.last_name %></td>
			<td><%= respondent.email %></td>
		</tr>
		<% end %>
	</tbody>
</table>
